    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vitest": "^0.34.6",
    "@vitest/ui": "^0.34.6",
